{
  "name": "sms-data-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "docker-compose -f docker-compose.dev.yml up --build",
    "down": "docker-compose -f docker-compose.dev.yml down",
    "serve": "nodemon --config nodemon.json src/index.ts",
    "build": "rimraf ./dist && tsc",
    "start": "ts-node src/index.ts",
    "prod": "node dist/src/index.js",
    "migrate": "npx knex migrate:unlock --knexfile ./src/database/knexfile.ts && npx knex migrate:latest --knexfile ./src/database/knexfile.ts",
    "migrate:down": "npx knex migrate:down --knexfile ./src/database/knexfile.ts",
    "lint": "eslint --fix 'src/**/*.ts'",
    "pre-commit": "lint-staged --cwd ."
  },
  "dependencies": {
    "@babel/runtime": "^7.22.5",
    "@sentry/node": "^7.57.0",
    "@sentry/tracing": "^7.57.0",
    "@toruslabs/loglevel-sentry": "^5.0.0",
    "@types/joi": "^17.2.3",
    "axios": "^1.4.0",
    "celebrate": "^15.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "elliptic": "^6.5.4",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "hi-base32": "^0.5.1",
    "keccak256": "^1.0.6",
    "knex": "^2.4.2",
    "knex-tiny-logger": "^2.1.0",
    "libphonenumber-js": "^1.10.37",
    "loglevel": "^1.8.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "rate-limit-redis": "^3.0.2",
    "redis": "^4.6.7"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/register": "^7.22.5",
    "@toruslabs/eslint-config-node": "^2.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/elliptic": "^6.4.14",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.44.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-n": "^15.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=16",
    "npm": ">=7.x"
  },
  "lint-staged": {
    "!(*d).ts": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  }
}
